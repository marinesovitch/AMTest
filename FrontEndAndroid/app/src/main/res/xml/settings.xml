<?xml version="1.0" encoding="utf-8"?>
<androidx.preference.PreferenceScreen
	xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_height="match_parent"
	android:layout_width="match_parent">
	<androidx.preference.CheckBoxPreference
		android:key="show_params"
		android:title="@string/show_params_title"
		android:summary="@string/show_params_summary"
		android:defaultValue="true" />
	<androidx.preference.CheckBoxPreference
		android:key="enable_extra_gestures"
		android:title="@string/enable_extra_gestures_title"
		android:summary="@string/enable_extra_gestures_summary"
		android:defaultValue="true" />
</androidx.preference.PreferenceScreen>
